#include <stdio.h>

void print_sp (int i, int n) {
    for (int sp = 0; sp < n - i; sp++) {
        printf(" ");
    }
}

void print_pascal_row (int n) {
    int val = 1;
    for (int i = 0; i <= n; i++) {
        printf("%d ", val);
        val = val*(n - i) / (i + 1);
    }
    printf("\n");
}

int main() {
    int rows = 6;
    for (int i = 0; i < rows; i++) {
        print_sp (i,rows);
        print_pascal_row (i);
    }
    return 0;
}
