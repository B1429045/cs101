#include <stdio.h>

int main()
{
    printf("顯示整數            = %i\n",12);
    printf("顯示浮點數          = %f\n",1.23);
    printf("顯示字元            = %c\n",'A');
    printf("顯示字元編碼        = %d\n",'A');//By ASCII
    printf("顯示字串            = %s\n","I am a string.");
    printf("顯示八進位整數      = %o\n",17);// 1 2 3 4 5 6 7 ( 10 11 12 13 14 15 16 17 20)
    printf("顯示十進位整數      = %d\n",15);
    printf("顯示無符號十進位整數 = %u\n",15);
    printf("顯示十六進位整數    = %X\n",15);// 1 2 3 4 5 6 7 8 9 A B C D E F (10...1A 1B...)
    printf("顯示十六進位整數    = %x\n\n",15);//%X == 大寫 \ %x == 小寫
    
    printf("顯示二進位整數      = %d\n",0b11);//0b ==二進位
    printf("顯示八進位整數      = %d\n",070);//0 ==八進位
    printf("顯示十六進位整數    = %d\n",0XFF);//0X ==十六進位
    
    return 0;
}
